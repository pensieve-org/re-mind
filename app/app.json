{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication"
    ],
    "android": {
      "package": "com.remind"
    },
    "ios": {
      "bundleIdentifier": "com.remind",
      "googleServicesFile": "./GoogleService-Info.plist"
    }
  },
  "ios": {
    "usesAppleSignIn": true
  }
}
