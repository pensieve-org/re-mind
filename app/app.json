{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      "@react-native-google-signin/google-signin"
    ],
    "android": {
      "package": "com.pensieve.remind"
    },
    "ios": {
      "bundleIdentifier": "com.pensieve.remind"
    }
  },
  "ios": {
    "usesAppleSignIn": true
  }
}
